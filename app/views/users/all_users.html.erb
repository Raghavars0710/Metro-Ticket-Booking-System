<p id="notice"><%= notice %></p>

<table class="table table-striped" >
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Phone Number</th>
      <th>Address</th>
      <th>Date Of Birth</th>
      <th>Gender</th>
      <th>Role</th>
      <th colspan="3">Click Any Operations</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.contact_number %></td>
      <td><%= user.address %></td>
      <td><%= user.date_of_birth %></td>
      <td><%= user.gender %></td>
      <td><%= user.role %></td>
      <td><%= link_to 'Show', user_path(user),class:"btn btn-primary" %></td>
      <td><%= link_to 'See There Member', user_members_path(user) ,class:"btn btn-primary" %></td>
      <td><%= button_to( "Remove User", user_path(user), method: :delete ,class:"btn btn-danger") %></td>
    </tr>
    <% end %>
  </tbody>
</table>




